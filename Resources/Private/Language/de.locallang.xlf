<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<xliff version="1.0">
    <file target-language="de" datatype="plaintext" original="messages">
        <header/>
        <body>
            <!-- ext_conf_template.txt -->
            <trans-unit id="config.default.storagePageId">
                <target>Speicherseiten-ID: Die Seiten-ID, die zum Speichern der Universal Messenger Newsletter-Kanaldatensätze verwendet wird.</target>
            </trans-unit>
            <trans-unit id="config.default.pageType">
                <target>Seitentyp: Dieser Wert definiert den Seitentyp, der für Universal Messenger Newsletter-Seiten verwendet wird. Er wird für das doktype-Feld der Seitentabelle in Ihrer Instanz verwendet. Stellen Sie hier einen anderen Wert ein, wenn der Standardwert bereits für andere Dinge verwendet wird.</target>
            </trans-unit>
            <trans-unit id="config.webservice.apiUrl">
                <target>Basis-URL: Die allgemeine Universal Messenger API-URL, die die Grundlage aller Requests ist, z.B. https://your-domain.td.universal-messenger.de/p.</target>
            </trans-unit>
            <trans-unit id="config.webservice.apiKey">
                <target>API-Schlüssel: Der Universal Messenger-API-Schlüssel</target>
            </trans-unit>
            <trans-unit id="config.webservice.logging">
                <target>Protokollierung aktivieren: Protokollieren Sie alle Universal Messenger API-Anfragen in einer Protokolldatei. Dazu konfigurieren Sie bitte einen entsprechenden Log-Schreiber.</target>
            </trans-unit>
            <trans-unit id="config.expert.testChannelSuffix">
                <target>Test-Newsletter-Kanal Suffix: Tragen Sie hier den Suffix für die TEST-Newsletter-Kanäle ein.</target>
            </trans-unit>
            <trans-unit id="config.expert.liveChannelSuffix">
                <target>Live-Newsletter-Kanal Suffix: Tragen Sie hier den Suffix für die LIVE-Newsletter-Kanäle ein.</target>
            </trans-unit>

            <!-- common -->
            <trans-unit id="common.universalMessenger">
                <target>Universal Messenger</target>
            </trans-unit>
            <trans-unit id="common.newsletterPreview">
                <target>Vorschau des Newsletters</target>
            </trans-unit>
            <trans-unit id="common.newsletterChannel">
                <target>Newsletter-Kanal</target>
            </trans-unit>
            <trans-unit id="common.newsletterSend">
                <target>Newsletter-Versand</target>
            </trans-unit>
            <trans-unit id="common.sendNewsletterTest">
                <target>Testversand</target>
            </trans-unit>
            <trans-unit id="common.sendNewsletterLive">
                <target>Live-Versand</target>
            </trans-unit>
            <trans-unit id="common.sendNewsletterLiveDialogContent">
                <target>Es erfolgt ein Live-Versand an den Kanal "%s". Möchten Sie dies wirklich?</target>
            </trans-unit>
            <trans-unit id="common.sendNewsletterLiveButtonCancel">
                <target>Abbrechen</target>
            </trans-unit>
            <trans-unit id="common.sendNewsletterLiveButtonConfirm">
                <target>Bestätigen</target>
            </trans-unit>

            <!-- errors -->
            <trans-unit id="error.missingTranslation">
                <target>Fehlende Übersetzung für:</target>
            </trans-unit>
            <trans-unit id="error.pageNotAllowed">
                <target>Bitte wählen Sie eine Seite vom Seiten-Typ "Newsletter" aus.</target>
            </trans-unit>
            <trans-unit id="error.accessNotAllowed">
                <target>Sie oder ihre Benutzergruppe verfügen nicht über die notwendigen Rechte, um diesen Newsletter zu versenden.</target>
            </trans-unit>
            <trans-unit id="error.noSiteConfiguration">
                <target>Für die angeforderte Seite fehlt eine gültige Seitenkonfiguration.</target>
            </trans-unit>
            <trans-unit id="error.invalidRequest">
                <target>Die übermittelte Anfrage ist ungültig.</target>
            </trans-unit>
            <trans-unit id="error.exceptionDuringCreate">
                <target>Bei der Generierung des Universal Messenger Newsletter-Event-Requests ist ein Fehler aufgetreten.</target>
            </trans-unit>
            <trans-unit id="error.missingChannelConfiguration">
                <target>Bitte wählen Sie einen gültigen Newsletter-Kanal in den Seiteneigenschaften aus.</target>
            </trans-unit>

            <!-- status -->
            <trans-unit id="newsletter.status.hold">
                <target>Der Newsletter wurde an den Universal Messenger übergeben und wird verarbeitet.</target>
            </trans-unit>
            <trans-unit id="newsletter.status.failed">
                <target>Der Newsletterversand ist fehlgeschlagen.</target>
            </trans-unit>
            <trans-unit id="newsletter.status.stopped">
                <target>Der Newsletterversand wurde gestoppt.</target>
            </trans-unit>
            <trans-unit id="newsletter.status.queue">
                <target>Der Newsletter befindet sich derzeit in der Warteschlange und der Versand hat noch nicht begonnen.</target>
            </trans-unit>
            <trans-unit id="newsletter.status.finished">
                <target>Der Newsletterversand ist abgeschlossen: Es wurde ein Benutzer kontaktiert.</target>
            </trans-unit>
            <trans-unit id="newsletter.status.finished.plural">
                <target>Der Newsletterversand ist abgeschlossen: Es wurden %d Benutzer kontaktiert.</target>
            </trans-unit>

            <!-- be_groups -->
            <trans-unit id="be_groups.universal_messenger_channels">
                <target>Newsletter-Kanäle</target>
            </trans-unit>
            <trans-unit id="be_groups.universal_messenger_channels.description">
                <target>Wählen Sie hier die Newsletter-Kanäle, für welche diese Benutzergruppe Newsletter versenden darf.</target>
            </trans-unit>

            <!-- be_users -->
            <trans-unit id="be_users.universal_messenger_channels">
                <target>Newsletter-Kanäle</target>
            </trans-unit>
            <trans-unit id="be_users.universal_messenger_channels.description">
                <target>Wählen Sie hier die Newsletter-Kanäle, für welche dieser Benutzer Newsletter versenden darf.</target>
            </trans-unit>

            <!-- pages -->
            <trans-unit id="pages.page_type_newsletter">
                <target>Newsletter</target>
            </trans-unit>
            <trans-unit id="pages.universal_messenger_channel">
                <target>Newsletter-Kanal</target>
            </trans-unit>
            <trans-unit id="pages.universal_messenger_channel.description">
                <target>Wählen Sie hier den Newsletter-Kanal für diesen Newsletter.</target>
            </trans-unit>
            <trans-unit id="pages.tx_universalmessenger_domain_model_newsletterchannel.0">
                <target>Bitte wählen Sie einen Kanal</target>
            </trans-unit>

            <!-- tx_universalmessenger_domain_model_newsletterchannel -->
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel">
                <target>Newsletter-Kanal</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.channel_id">
                <target>Newsletter-Kanal-ID</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.channel_id.description">
                <target>Die eindeutige ID des Newsletter-Kanals.</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.title">
                <target>Titel</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.title.description">
                <target>Der Titel des Newsletter-Kanals.</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.description">
                <target>Beschreibung</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.sender">
                <target>E-Mail-Adresse des Absenders</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.sender.description">
                <target>Die E-Mail-Adresse des Absenders. Wenn gesetzt, so überschreibt dies die in der Konfigurationsdatei voreingestellte Absender-ID.</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.reply_to">
                <target>E-Mail-Adresse für Antwort</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.reply_to.description">
                <target>Die Antwort-E-Mail-Adresse. Wenn gesetzt, so überschreibt dies die in der Konfigurationsdatei voreingestellte Antwort-ID.</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.skip_used_id">
                <target>Verwendete ID überspringen</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.skip_used_id.description">
                <target>Aktivieren Sie diese Option, wenn der LIVE-Versand abgebrochen werden soll, wenn bereits ein Newsletter mit derselben Ereignis-ID im Archiv vorhanden ist. Die Ereignis-ID wird automatisch aus der Kennung der Seitenkonfiguration, der ID der gewählten Sprache sowie der Seiten-ID generiert. Der Testversand ist unabhängig von der Auswahl immer möglich.</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images">
                <target>Bilder einbetten</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images.description">
                <target>Verhalten für das Einbetten von Bildern.</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images.all">
                <target>Alle Bilder in die E-Mail einbetten</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images.byPath">
                <target>Alle Bilder mit einem relativen Link einbetten</target>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images.none">
                <target>Keine Bilder einbetten</target>
            </trans-unit>
        </body>
    </file>
</xliff>
