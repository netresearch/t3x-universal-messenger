<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<xliff version="1.0">
    <file source-language="en" datatype="plaintext" original="messages">
        <header/>
        <body>
            <!-- ext_conf_template.txt -->
            <trans-unit id="config.default.storagePageId">
                <source>Storage page ID: The page ID used to store the Universal Messenger newsletter channel records.</source>
            </trans-unit>
            <trans-unit id="config.default.pageType">
                <source>Page type: This value defines the page type used for Universal Messenger newsletter pages. It is used for the doctype field of the page table in your instance. Set a different value here if the default value is already used for other things.</source>
            </trans-unit>
            <trans-unit id="config.webservice.apiUrl">
                <source>Base URL: The general Universal Messenger API URL, which is the basis of all requests, e.g. https://your-domain.td.universal-messenger.de/p</source>
            </trans-unit>
            <trans-unit id="config.webservice.apiKey">
                <source>API key: The Universal Messenger API Key</source>
            </trans-unit>
            <trans-unit id="config.webservice.logging">
                <source>Enable logging: Log all Universal Messenger API requests in a log file. To do this, please configure an appropriate log writer.</source>
            </trans-unit>
            <trans-unit id="config.expert.testChannelSuffix">
                <source>Test newsletter channel suffix: Enter the suffix for the TEST newsletter channels here.</source>
            </trans-unit>
            <trans-unit id="config.expert.liveChannelSuffix">
                <source>Live newsletter channel suffix: Enter the suffix for the LIVE newsletter channels here.</source>
            </trans-unit>

            <!-- common -->
            <trans-unit id="common.universalMessenger">
                <source>Universal Messenger</source>
            </trans-unit>
            <trans-unit id="common.newsletterPreview">
                <source>Preview of the newsletter</source>
            </trans-unit>
            <trans-unit id="common.newsletterChannel">
                <source>Newsletter channel</source>
            </trans-unit>
            <trans-unit id="common.newsletterSend">
                <source>Newsletter dispatch</source>
            </trans-unit>
            <trans-unit id="common.sendNewsletterTest">
                <source>Test delivery</source>
            </trans-unit>
            <trans-unit id="common.sendNewsletterLive">
                <source>Live delivery</source>
            </trans-unit>
            <trans-unit id="common.sendNewsletterLiveDialogContent">
                <source>A live delivery will be sent to channel "%s". Are you sure you want to do this?</source>
            </trans-unit>
            <trans-unit id="common.sendNewsletterLiveButtonCancel">
                <source>Cancel</source>
            </trans-unit>
            <trans-unit id="common.sendNewsletterLiveButtonConfirm">
                <source>Confirm</source>
            </trans-unit>

            <!-- errors -->
            <trans-unit id="error.missingTranslation">
                <source>Missing translation for:</source>
            </trans-unit>
            <trans-unit id="error.pageNotAllowed">
                <source>Please select a page with the page type "Newsletter".</source>
            </trans-unit>
            <trans-unit id="error.accessNotAllowed">
                <source>You or your user group do not have the necessary rights to send this newsletter.</source>
            </trans-unit>
            <trans-unit id="error.noSiteConfiguration">
                <source>The requested page is missing a valid site configuration.</source>
            </trans-unit>
            <trans-unit id="error.invalidRequest">
                <source>The submitted request is invalid.</source>
            </trans-unit>
            <trans-unit id="error.exceptionDuringCreate">
                <source>An error occurred while generating the Universal Messenger newsletter event request.</source>
            </trans-unit>
            <trans-unit id="error.missingChannelConfiguration">
                <source>Please select a valid newsletter channel in the page properties.</source>
            </trans-unit>
            <trans-unit id="error.pageHidden">
                <source>Please select a non disabled page.</source>
            </trans-unit>

            <!-- status -->
            <trans-unit id="newsletter.status.hold">
                <source>The newsletter has been sent to Universal Messenger and is being processed.</source>
            </trans-unit>
            <trans-unit id="newsletter.status.failed">
                <source>Newsletter sending has failed.</source>
            </trans-unit>
            <trans-unit id="newsletter.status.stopped">
                <source>Newsletter sending has been stopped.</source>
            </trans-unit>
            <trans-unit id="newsletter.status.queue">
                <source>Newsletter is currently in the queue and sending has not started yet.</source>
            </trans-unit>
            <trans-unit id="newsletter.status.finished">
                <source>The newsletter has been sent out: One user has been contacted.</source>
            </trans-unit>
            <trans-unit id="newsletter.status.finished.plural">
                <source>The newsletter has been sent out: %d users have been contacted.</source>
            </trans-unit>

            <!-- be_groups -->
            <trans-unit id="be_groups.universal_messenger_channels">
                <source>Newsletter channels</source>
            </trans-unit>
            <trans-unit id="be_groups.universal_messenger_channels.description">
                <source>Select the newsletter channels for which this user group is allowed to send newsletters.</source>
            </trans-unit>

            <!-- be_users -->
            <trans-unit id="be_users.universal_messenger_channels">
                <source>Newsletter channels</source>
            </trans-unit>
            <trans-unit id="be_users.universal_messenger_channels.description">
                <source>Select the newsletter channels for which this user is allowed to send newsletters.</source>
            </trans-unit>

            <!-- pages -->
            <trans-unit id="pages.page_type_newsletter">
                <source>Newsletter</source>
            </trans-unit>
            <trans-unit id="pages.universal_messenger_channel">
                <source>Newsletter channel</source>
            </trans-unit>
            <trans-unit id="pages.universal_messenger_channel.description">
                <source>Select the newsletter channel for this newsletter here.</source>
            </trans-unit>
            <trans-unit id="pages.tx_universalmessenger_domain_model_newsletterchannel.0">
                <source>Please select a channel</source>
            </trans-unit>

            <!-- tx_universalmessenger_domain_model_newsletterchannel -->
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel">
                <source>Newsletter channel</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.channel_id">
                <source>Newsletter channel ID</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.channel_id.description">
                <source>The unique ID of the newsletter channel.</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.title">
                <source>Title</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.title.description">
                <source>The title the newsletter channel.</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.description">
                <source>Description</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.sender">
                <source>Sender email address</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.sender.description">
                <source>The sender's email address. If set, this overwrites the sender ID preset in the configuration file.</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.reply_to">
                <source>Reply-to email address</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.reply_to.description">
                <source>The reply-to email address. If set, this overwrites the reply-to ID preset in the configuration file.</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.skip_used_id">
                <source>Skip used ID</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.skip_used_id.description">
                <source>Activate this option if you want the LIVE mailing to be canceled if a newsletter with the same event ID already exists in the archive. The event ID is automatically generated from the site identifier, the ID of the selected language and the page ID. Test mailing is always possible regardless of the selection.</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images">
                <source>Embed images</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images.description">
                <source>Behavior for embedding images.</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images.all">
                <source>Embed all images in the email</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images.byPath">
                <source>Embed all images with a relative link</source>
            </trans-unit>
            <trans-unit id="tx_universalmessenger_domain_model_newsletterchannel.embed_images.none">
                <source>Embed no images</source>
            </trans-unit>
        </body>
